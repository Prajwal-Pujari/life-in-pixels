```typescript
import React from 'react';
import DayBox from './DayBox';

interface MonthGridProps {
  currentDate: Date;
  onDayClick: (year: number, month: number, day: number) => void;
  entries: Record<string, any>; // Changed from Record<string, string> to accept JournalEntry objects
  holidays: Record<string, string>;
}

const MonthGrid: React.FC<MonthGridProps> = ({ currentDate, onDayClick, entries, holidays = {} }) => {
  const year = currentDate.getFullYear();
  const month = currentDate.getMonth();
  const today = new Date(); // Assuming 'today' refers to the actual current date, not the date passed in props

  const firstDay = new Date(year, month, 1);
  const lastDay = new Date(year, month + 1, 0);
  const daysInMonth = lastDay.getDate();
  const startWeekday = firstDay.getDay();

  const getDayState = (day: number): 'past' | 'today' | 'future' | 'holiday' => {
    const currentDate = new Date(year, month, day);
    const todayDate = new Date(today.getFullYear(), today.getMonth(), today.getDate());
    
    // Check if it's a holiday first
    const dayKey = `${ year } -${ month } -${ day } `;
    if (holidays[dayKey]) return 'holiday';
    
    if (currentDate < todayDate) return 'past';
    if (currentDate.getTime() === todayDate.getTime()) return 'today';
    return 'future';
  };

  const getDayKey = (day: number) => `${ year } -${ month } -${ day } `;

  const days = [];
  
  for (let i = 0; i < startWeekday; i++) {
    days.push(<DayBox key={`placeholder - ${ i } `} day={null} state="placeholder" />);
  }
  
  for (let day = 1; day <= daysInMonth; day++) {
    const dayKey = getDayKey(day);
    const holidayName = holidays[dayKey];
    
    days.push(
      <DayBox 
        key={day} 
        day={day} 
        state={getDayState(day)}
        onClick={() => onDayClick(year, month, day)}
        hasEntry={!!entries[dayKey]}
        holidayName={holidayName}
      />
    );
  }

  const monthNames = [
    'JANUARY', 'FEBRUARY', 'MARCH', 'APRIL', 'MAY', 'JUNE',
    'JULY', 'AUGUST', 'SEPTEMBER', 'OCTOBER', 'NOVEMBER', 'DECEMBER'
  ];

  const weekdays = ['SUN', 'MON', 'TUE', 'WED', 'THU', 'FRI', 'SAT'];

  return (
    <div className="month-grid">
      <h2 className="month-title">{monthNames[month]} {year}</h2>
      <div className="weekday-labels">
        {weekdays.map(day => (
          <div key={day} className="weekday-label">{day}</div>
        ))}
      </div>
      <div className="calendar-grid">
        {days}
      </div>
    </div>
  );
};

export default MonthGrid;
